{% extends "base.twig" %}

{% block spec_styles %}
  <!-- shop styles -->
  <link rel="stylesheet" href="{{theme.link|e('esc_url')}}/assets/css/account.css" />
{% endblock %}

{% block pageheader %}
  {% include 'header-page.twig' %}
{% endblock %}

{% block contentwrap %}
  <div class="uk-container" data-template="account.twig" data-block="contentwrap">
    {{ post.content }}
  </div>
{% endblock %}

{% block abovefooter %}
  {% include 'above-footer-page.twig' %}
{% endblock %}

{% block spec_scripts %}
  <script type='text/javascript' src='{{theme.link|e('esc_url')}}/assets/js/woo/my-account.js'></script>
{% endblock %}