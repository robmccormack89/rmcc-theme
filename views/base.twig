<!DOCTYPE html>
<html {{site.language_attributes}}>

<head>
  <meta charset="{{site.charset}}" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  
  {{ function('wp_head') }}

  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@800&family=Open+Sans:ital,wght@0,600;0,800;1,400&display=swap" rel="stylesheet">
  
  {{options.header_embed_codes}}
  
  <!-- styles per template -->
  {% block spec_styles %}
  {% endblock %}
  <!-- /styles per template -->
  
  <style>
    .uk-overlay-primary {
        background: rgb(10 10 10 / 80%);
    }
  </style>
  
</head>

{% block bodywrap %}

<body class="{{ body_class }} no-overflow-off" data-template="base.twig">
  <div class="theme-preload-off"></div>

  <div id="SiteWrapper" class="site-wrapper">

    {% block header %}
      {% include 'header.twig' %}
    {% endblock %}
    
    <main role="main" id="MainContent" class="main_content">
      {% block content %}
        <div class="">
          <div class="uk-background-muted">
            <div class="breads_area uk-container">
              {% include 'breads.twig' %}
            </div>
          </div>
          <div class="content-area uk-section uk-section-small uk-section-muted">
            <div class="uk-container uk-margin-large-bottom">
              {% block contentwrap %}
                <div class="">
                  <h1 class="uk-heading-line uk-text-bold">
                    <span>{{ title }}</span>
                  </h1>
                </div>
                <div class="uk-card uk-card-default uk-card-body">
                  {% block contentonly %}
                    <p>Sorry, Nothing to Display!!</p>
                  {% endblock %}
                </div>
              {% endblock %}
            </div>
          </div>
        </div>
      {% endblock %}
    </main>
    
    {% block footer %}
      {% include 'footer.twig' %}
    {% endblock %}

  </div>
  
  {{ function('wp_footer') }}

  {% if is_paginated %}
    <script type="text/javascript" src="/wp-content/themes/rmcc-woo-theme/assets/js/main/scroll.js"></script>
  {% endif %}
  
  <!-- scripts per template -->
  {% block spec_scripts %}
  {% endblock %}
  <!-- /styles per template -->
  
  <script>
    // jQuery(function($) {
        
    //     window.addEventListener("pageshow", function() {
    //         $('.theme-preload').hide('fast');
    //         $('body').removeClass('no-overflow');
    //     }, false);
      
    // });
  </script>
  
</body>
<!-- /body -->

{% endblock %}

</html>