{% extends "minimal-page.twig" %}

{% block contentwrap %}
  <section class="uk-section">
    <div class="single-content uk-text-center uk-container uk-container-xsmall">
      <h1>
        {{ _x('404 Page Not Found', '404 Error page title', 'cautious-octo-fiesta')|e('esc_html') }}
      </h1>
      <p>
        {{ _x('The page you requested does not exist.', '404 Error page message', 'cautious-octo-fiesta')|e('esc_html') }}
      </p>
      <a href="/" class="uk-button uk-button-primary uk-button-large">
        {{ _x('Back to Homepage', '404 Error page Back to Homepage text', 'cautious-octo-fiesta')|e('esc_html') }}
      </a>
    </div>
  </section>
{% endblock %}