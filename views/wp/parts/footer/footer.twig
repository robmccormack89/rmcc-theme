<footer>
  
  {# include 'footer-secondary.twig' #}
  
  <div id="FooterMain" class="uk-background-secondary uk-section uk-padding-remove-bottom uk-light">
    <div class="uk-container uk-container-expand">
      
      <div class=" uk-margin-bottom uk-grid-large" uk-grid>
        
        <div class="uk-width-1-3@m">
          
          <div class="footer-logo uk-margin-bottom">
            <a href="{{ site.url|e('esc_url') }}" title="{{ site.title|e('html_attr') }}">
              <img class="uk-logo" data-src="{{theme.link}}/assets/images/sixstar-logo-darkbg.png" width="250" height="82" alt="{{ site.title|e('esc_html') }} Logo" uk-img>
            </a>
          </div>
          
          <div class="footer-left-content">
            {% if sidebar_footer_left %}
              {{ sidebar_footer_left }}
            {% else %}
              <p class="uk-text-emphasis">Aliquam lorem ante, dapibus in, viverra quis, feugiat a, tellus. Phasellus viverra nulla ut metus varius laoreet. Quisque rutrum.</p>
              <a href="#" class="uk-text-underline">Click here</a>
            {% endif %}
          </div>
          
        </div>
        
        <div class="uk-width-1-6@m">
          {% if has_footer_nav_menu %}
            <h3 class="uk-text-uppercase uk-h4">{{footer_nav_menu.title|e('esc_html')}}</h3>
            <ul class="uk-nav-default" uk-nav>
              {% for item in footer_nav_menu.get_items %}
                <li class="{{ item.classes | join(' ')|e('html_attr') }}">
                  <a href="{{ item.get_link|e('esc_url') }}">{{ item.title|e('esc_html') }}</a>
                </li>
              {% endfor %}
            </ul>
          {% else %}
            <h3 class="uk-text-uppercase uk-h4">Navigation</h3>
            <ul class="uk-nav-default" uk-nav>
              <li><a href="#">Lorem</a></li>
              <li><a href="#">Aliquam lorem ante</a></li>
              <li><a href="#">Phasellus viverra nulla</a></li>
              <li><a href="#">Quisque rutrum tellus</a></li>
              <li><a href="#">Aliquam lorem</a></li>
            </ul>
          {% endif %}
        </div>
        
        <div class="uk-width-1-6@m">
          {% if has_footer_customers_menu %}
            <h3 class="uk-text-uppercase uk-h4">{{footer_customers_menu.title|e('esc_html')}}</h3>
            <ul class="uk-nav-default" uk-nav>
              {% for item in footer_customers_menu.get_items %}
                <li class="{{ item.classes | join(' ')|e('html_attr') }}">
                  <a href="{{ item.get_link|e('esc_url') }}">{{ item.title|e('esc_html') }}</a>
                </li>
              {% endfor %}
            </ul>
          {% else %}
            <h3 class="uk-text-uppercase uk-h4">Customers</h3>
            <ul class="uk-nav-default" uk-nav>
              <li><a href="#">Lorem</a></li>
              <li><a href="#">Aliquam lorem ante</a></li>
              <li><a href="#">Viverra</a></li>
              <li><a href="#">Quisque rutrum</a></li>
            </ul>
          {% endif %}
        </div>
        
        <div class="uk-width-1-3@m uk-text-left uk-text-right@m">
          <h3 class="uk-text-uppercase uk-h4">{{ __('Sales & Suppport', 'urban-carnival-theme') }}</h3>
          <div class="widget-content">
            <p class="uk-margin-remove-bottom">
              <a href="tel:{{company_phone_number|e('html_attr')|default('+01 234 5678')}}">
                {{ __('Call') }} {{company_phone_number|e('esc_html')|default('+01 234 5678')}}
              </a> or 
              <a href="mailto:{{display_email|e('html_attr')|default('info@hello.com')}}">
                {{ __('Email us', 'urban-carnival-theme') }}
              </a>
            </p>
            <p class="uk-margin-remove-top uk-text-muted">
              {{base_address|e('esc_html')|default('Some Street')}}, {{base_address_2|e('esc_html')|default('Some Place')}},<br>
              {{base_city|e('esc_html')|default('Some City')}}, {{base_county|e('esc_html')|default('Some County')}},<br>
              {{base_eircode|e('esc_html')|default('Some Eircode')}}
            </p>
          </div>
        </div>
        
      </div>

      <div class="uk-margin-medium-top uk-margin-medium-bottom uk-child-width-1-2@m uk-flex uk-flex-bottom" uk-grid>
        <div class="uk-text-center uk-text-left@m">
          {% include 'payment-icons.twig' %}
        </div>
        <div class="uk-text-center uk-text-right@m">
          <div class="footer-copyright">
            Â© {{ now|date('Y')|e('esc_html') }} {{ bloginfo('name')|e('esc_html') }} | Developed by <a href="https://github.com/robmccormack89" target="_blank">RMcCormack</a>
          </div>
        </div>
      </div>
      
    </div>
  </div>
  
</footer>