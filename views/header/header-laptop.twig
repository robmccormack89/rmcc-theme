<div id="HeaderBottomLaptop" class="uk-visible@m uk-hidden@xl fat-border-bottom">
  <div id="BottomNavLaptop" class="">
    <nav id="BottomNavWrapLaptop" class="theme-dark-bg">
      <div class="uk-container uk-container-expand container-wrap-10">
        <div class="" uk-grid>
          <div class="uk-width-auto">
            <div class="uk-navbar" uk-navbar>
              <div id="BottomNavLogoLaptop" class="bottom_nav_logo bottom_nav_logo_laptop uk-navbar-left uk-light">
                <a class="uk-navbar-item uk-logo uk-text-bold uk-text-emphasis" href="/"><img data-src="/wp-content/uploads/2020/02/sixstar-logo-red.png" uk-img=""
                    src="/wp-content/uploads/2020/02/sixstar-logo-red.png"></a>
              </div>
            </div>
          </div>
          <div class="uk-width-expand">
            <div class="top-block">
              <div class="uk-navbar" uk-navbar>
                <div id="BottomNavRightLaptop" class="bottom_nav_right bottom_nav_right_laptop uk-navbar-right search-overlay-laptop navbar-height-full">
                  <div id="SearchNavLaptop" class="search_nav uk-navbar-item uk-light">
                    <a href="#" uk-icon="icon:search;ratio:1.1" class="nav-right-item lighten" uk-toggle="target: .search-overlay-laptop; animation: uk-animation-fade"></a>
                  </div>
                  <div id="CartNavLaptop" class="cart_nav cart_nav_mobile uk-navbar-item boundary-cart-laptop">
                    <div class="uk-inline">
                      <a href="/cart" uk-icon="icon:cart;ratio:1.3" class="cart-icon uk-icon nav-right-item uk-light lighten" aria-expanded="false"></a>
                      {% do action('cart_ajax_result_count') %}
                      {% do action('cart_ajax_subtotal') %}
                    </div>
                  </div>
                  <div id="CartNavDropdownLaptop" class="uk-padding-remove" uk-dropdown="pos: bottom-right; boundary: .boundary-cart-laptop; boundary-align: true">
                    <div class="widget_shopping_cart_content">{{ function('woocommerce_mini_cart') }}</div>
                  </div>
                  <div id="BottomNavRightDividerLaptop" class="bottom_nav_right_divider uk-navbar-item uk-light">
                    <hr class="uk-divider-vertical height-50 uk-margin-remove">
                  </div>
                  <div id="AccountNavLaptop" class="account_nav uk-navbar-item boundary-account-laptop">
                    <a href="{{dashboard_endpoint}}" uk-icon="icon:user;ratio:1.2" class="nav-right-item uk-light lighten" aria-expanded="false"></a>
                  </div>
                  <div id="AccountNavDropdownLaptop" uk-dropdown="pos: bottom-right; boundary: .boundary-account-laptop; boundary-align: true">
                    {% if not user %}
                    <ul id="LoginDropdownLaptop" class="login_dropdown uk-nav uk-dropdown-nav">
                      <li><a href="{{dashboard_endpoint}}" class="uk-text-uppercase grey-text font-600">Login/Register</a></li>
                    </ul>
                    {% else %}
                    <ul id="AccountDropdownLaptop" class="uk-nav uk-dropdown-nav">
                      <li class="uk-nav-header uk-text-small uk-text-bold">YOUR ACCOUNT</li>
                      <li><a href="{{dashboard_endpoint}}"><span data-uk-icon="icon: user" class="uk-icon"></span> Dashboard</a></li>
                      <li><a href="{{address_endpoint}}"><span data-uk-icon="icon: location" class="uk-icon"></span> Change Address</a></li>
                      <li><a href="{{edit_endpoint}}"><span data-uk-icon="icon: lock" class="uk-icon"></span> Change Password</a></li>
                      <li class="uk-nav-divider"></li>
                      <li><a href="{{orders_endpoint}}"><span data-uk-icon="icon: list" class="uk-icon"></span> Your Orders</a></li>
                      <li><a href="/contact"><span data-uk-icon="icon: mail" class="uk-icon"></span> Contact us</a></li>
                      <li class="uk-nav-divider"></li>
                      <li><a href="{{logout_endpoint}}"><span data-uk-icon="icon: sign-out" class="uk-icon"></span> Logout</a></li>
                    </ul>
                    {% endif %}
                  </div>
                </div>
                <div id="BottomNavSearchbarLaptop" class="bottom_nav_searchbar uk-navbar-right search-overlay-laptop" hidden>
                  <div class="uk-navbar-item">
                    <form class="uk-search uk-search-navbar" role="search" method="get" action="{{site.url}}">
                      <input class="uk-search-input" type="search" placeholder="Start typing..." id="Search" name="s" autocomplete="off" autofocus>
                    </form>
                  </div>
                  <a class="uk-navbar-toggle" uk-close uk-toggle="target: .search-overlay-laptop; animation: uk-animation-fade" href="#"></a>
                </div>
              </div>
            </div>
            <div class="bottom-block">
              <div class="uk-navbar" uk-navbar>
                <div class="bottom_nav_center bottom_nav_right_main_laptop uk-navbar-right search-overlay-laptop navbar-height-45 uk-light">
                  <ul class="uk-navbar-nav">
                    {% if has_menu_main %}
                    {% for item in menu_main.get_items %}
                    {% if item.get_children %}
                    <li class="{{ item.classes | join(' ') }}"><a href="{{ item.link }}">{{ item.title }}<span data-uk-icon="icon: triangle-down"></span></a>
                      <div class="uk-navbar-dropdown">
                        <ul class="uk-nav uk-navbar-dropdown-nav">
                          {% for child in item.get_children %}
                          <li><a href="{{ child.get_link }}">{{ child.title }}</a>
                            {% if child.children %}
                            <ul class="uk-nav-sub">
                              {% for third in child.children %}
                              <li>
                                <a href="{{ third.get_link }}">{{ third.title }}</a>
                              </li>
                              {% endfor %}
                            </ul>
                            {% endif %}
                          </li>
                          {% endfor %}
                        </ul>
                      </div>
                    </li>
                    {% else %}
                    <li class="{{ item.classes | join(' ') }} uk-inline"><a href="{{ item.get_link }}">{{ item.title }}</a></li>
                    {% endif %}
                    {% endfor %}
                    {% else %}
                    <li>Add a menu to the Main Menu slot in Appearance > Menus.</li>
                    {% endif %}
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </nav>
  </div>
</div>