<!DOCTYPE html>
<html {{site.language_attributes}} class="dark-style uk-light">

<head>
  <meta charset="{{site.charset}}" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- wp header -->
  {{ function('wp_head') }}
  <!-- /wp header -->
  <style>
    ul.tickets_numbers li.tn {
        background-color: #00BCD4;
    }
    ul.tickets_numbers li.selected {
        background: #4CAF50 !important;
    }
    ul.lottery-pn-answers li.selected {
        background: #4CAF50;
    }
    ul.lottery-pn-answers li {
        background-color: #00BCD4;
    }
    ul.lottery-pn-answers li.selected.false {
        background: #F44336;
    }
    #HomeSlider .uk-overlay-primary {
      background: #101010e0;
    }
    #HomeSlider .uk-overlay {
        padding: 20px;
    }
    @media only screen and (max-device-width:767px) {
      #HomeSlider .uk-overlay {
        padding: 10px;
      }
    }
    .single-product-image {
        height: 150px!important;
        object-fit: cover;
        width: 100%;
    }
    /* footer */
    .organic-social-icons a.icon {
      padding: 5px;
      background-color: rgba(92,92,92,0.15);
      border-radius: 100%;
      transition: background-color 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94), color 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
    }
    .organic-social-icons a.icon:hover {
      padding: 5px;
      background-color: rgba(92,92,92,0.05);
      color: #fc4c02!important;
    }
    #FooterBottomRight .uk-subnav>* {
      -webkit-box-flex: 0;
      flex: none;
      padding-left: 10px;
      position: relative;
    }
    .footer-padding {
        padding: 40px 0 20px;
    }
    .uk-light #SiteHeader .uk-navbar-nav>li>a {
        color: hsla(0,0%,100%,.7);
    }
    
    .uk-light #SiteHeader .uk-navbar-nav>li>a:hover {
        color: hsla(0,0%,100%,.9);
    }
    
    #SiteHeader .uk-navbar-item, #SiteHeader .uk-navbar-nav>li>a, #SiteHeader .uk-navbar-toggle {
        min-height: 0;
    }
    
    #SiteHeader {
      padding: 10px 0 10px;
      transition: background ease 0.2s
    }
    #SiteHeader.uk-sticky-below {
        background: #060606e0;
    }
    .uk-accordion-title:focus,
    .uk-accordion-title:hover {
      color: #fff;
    }

    .uk-light .uk-tab>.uk-active>a {
      border-color: #fc4c02;
    }

    .woocommerce #content div.product div.images,
    .woocommerce-page #content div.product div.images,
    .woocommerce-page div.product div.images,
    .woocommerce div.product div.images {
      width: 100%;
    }

    #breadcrumbs {
      color: #fff;
    }

    #breadcrumbs a {
      color: #fc4c0c;
      font-weight: bold;
    }

    .grid-item.product .uk-card {
      padding: 15px;
    }

    .sixstar-border {
      border: 1px solid #fc4c02;
    }
    #ProductTitle .uk-card-title {
        font-size: 1.3rem;
        line-height: 1.3;
    }

    #ProductInfo.uk-padding-small {
      padding: 15px 0;
    }

    .uk-label {
      background-color: #fc4c02 !important;
      color: #fff !important;
      font-weight: 600;
    }

    .dark-style body {
      background-image: url(/wp-content/themes/sixstar-theme/assets/img/dark_mosaic.png);
    }

    .dark-style .uk-background-muted,
    .dark-style .uk-card-default,
    .dark-style .uk-comment-primary,
    .dark-style .uk-section-muted,
    .dark-style .uk-table-striped>tr:nth-of-type(odd),
    .dark-style .uk-table-striped tbody tr:nth-of-type(odd),
    .dark-style .uk-tile-muted {
      background: #1b1b1b87;
    }

    .uk-button {
      font-weight: 600 !important;
    }

    .uk-light .uk-nav-default>li>a:hover {
      color: #fc4c02;
    }

    .uk-button-primary:disabled,
    .uk-button-secondary:disabled {
      background-color: transparent !important;
      color: #999 !important;
      border-color: #e5e5e5 !important;
    }

    ::selection {
      background: #fc4c02;
      color: #fff;
      text-shadow: none;
    }

    .tickets_numbers {
      margin-left: -0.5em !important;
    }

    .dark-style .uk-modal {
      background: rgba(53, 53, 53, 0.36);
    }

    .dark-style .uk-modal-dialog {
      background-color: #131313;
    }

    .dark-style .uk-modal-footer {
      background: #101010;
    }

    .uk-offcanvas-bar {
      position: absolute;
      top: 0;
      bottom: 0;
      left: -270px;
      box-sizing: border-box;
      width: 80vw;
      padding: 20px;
      background: #0c0c0c;
      overflow-y: auto;
      -webkit-overflow-scrolling: touch;
    }

    @media only screen and (max-width:479px) {
      .woocommerce div.product .entry-images div.images {
        width: 100%;
      }

      .uk-navbar-toggle {
        padding: 0 0 0 15px !important;
      }

      ul.lottery-pn-answers li {
        width: auto !important;
      }

      .uk-text-large {
        font-size: 1.2rem;
      }

      .uk-modal-body {
        padding: 15px;
      }
    }

    .uk-button {
      font-weight: 700;
    }

    .uk-button-large {
      font-size: 1.1rem;
    }

    .header-cart-count {
      display: inline-block;
      text-align: center;
      background-color: #fc4c02;
      color: #101010;
      font-weight: 700;
      font-size: 13px;
      min-width: 20px;
      height: 20px;
      line-height: 20px;
      border-radius: 20px;
      position: absolute;
      top: 15px;
      right: -5px;
    }

    .mobile-nav .header-cart-count {
      display: inline-block;
      text-align: center;
      background-color: #fc4c02;
      color: #101010;
      font-weight: 700;
      font-size: 13px;
      min-width: 20px;
      height: 20px;
      line-height: 20px;
      border-radius: 20px;
      position: absolute;
      top: 0px;
      right: -7px;
    }

    .cart-icon:hover {
      color: hsla(0, 0%, 100%, .7);
    }

    @media only screen and (max-device-width:767px) {
      .mobile-payment-icons {
        margin-left: -10px;
        margin-top: 20px;
      }
    }

    /* .uk-container {
        max-width: 1100px;
    } */
    /* utility classes */
    .padding-left-40 {
      padding-left: 40px;
    }

    .padding-top-20 {
      padding-top: 20px;
    }

    .padding-10-20-0-20 {
      padding: 10px 20px 0px 20px;
    }

    .height-content {
      min-height: fit-content !important;
    }

    .pull-right-30 {
      margin-right: -30px;
    }

    .pull-right-15 {
      margin-right: -15px;
    }

    .pull-right-10 {
      margin-right: -10px;
    }

    .pull-left-15 {
      margin-left: -15px;
    }

    .pull-left-10 {
      margin-left: -10px;
    }

    .push-left-7 {
      margin-left: 7px;
    }

    .darker-bg-primary {
      background-color: #20865f;
    }

    .padding-20 {
      padding: 20px;
    }

    .padding-20-0 {
      padding: 20px 0;
    }

    .padding-remove-bottom {
      padding-bottom: 0;
    }

    .padding-remove-top {
      padding-top: 0;
    }

    .padding-0-10 {
      padding: 0 10px;
    }

    .padding-top-20 {
      padding-top: 20px;
    }

    .padding-bottom-20 {
      padding-bottom: 20px;
    }

    .padding-0-0-0-20 {
      padding: 0 0 0 20px;
    }

    .dark-bg-black {
      background: #1f7b58;
    }

    .font-1-5 {
      font-size: 1.5rem !important;
    }

    .font-2 {
      font-size: 2rem;
    }

    .min-height-40 {
      min-height: 40px;
    }

    .width-49 {
      width: 49%;
    }

    .margin-left-2 {
      margin-left: 2%;
    }
  </style>
</head>

<!-- body classes go here -->

<body class="{{ body_class }} uk-background-center-center uk-background-fixed" data-template="base.twig">

  <!-- main site wrapper -->
  <div id="SiteWrapper" class="site-wrapper">

    <!-- header -->
    {% block header %}
    {% include 'header.twig' %}
    {% endblock %}
    <!-- /header -->

    <!-- content -->
    <main role="main" id="MainContent">
      {% block content %}
      <p>Sorry, Nothing to Display!!</p>
      {% endblock %}
    </main>
    <!-- /content -->

    <!-- footer -->
    {% block footer %}
    {% include 'footer.twig' %}
    {% endblock %}
    <!-- /footer -->

  </div>
  <!-- /main site wrapper -->

  <!-- wp footer -->
  {{ function('wp_footer') }}
  <!-- /wp footer -->

</body>
<!-- /body -->

</html>