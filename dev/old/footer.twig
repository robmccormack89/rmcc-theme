            <div class="uk-child-width-1-2@s uk-child-width-1-3@m uk-child-width-1-4@l uk-grid-small" uk-grid>
              <div>
                {% if sidebar_footer_1 %}
                {{ sidebar_footer_1 }}
                {% else %}
