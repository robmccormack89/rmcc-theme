{
  "name": "acf/card",
  "title": "RMcC Card",
  "description": "A card (rmcc-card) block built using ACF & Timber/Twig.",
  "category": "rmcc",
  "icon": "admin-comments",
  "keywords": ["rmcc", "card"],

  "acf": {
    "mode": "preview",
    "renderTemplate": "card.php"
  },
  
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "spacing": {

          "margin": {
            "bottom": "0rem",
            "top": "0rem"
          },

          "padding": "0rem"

        }
      },
      "___background": {
        "backgroundImage": {
          "url":"https://picsum.photos/1920/1200"
        },
        "backgroundPosition":"50% 50%",
        "backgroundSize": "contain"
      }
    }
  },

  "supports": {

    "__defaults": "",
    "html": false,
    "mode": true,
    "jsx": true,
    "anchor": true,
    "multiple": true,

    "__controls": "",
    "align": true,
    "fullHeight": true,
    "spacing": {
      "margin": ["bottom", "top"],
      "padding": true
    },
    "color": {
      "background": true,
      "gradients": true,
      "__not_working_yet": "",
      "text": false,
      "link": false,
      "heading": false,
      "button": false,
      "enableContrastChecker": false
    },

    "__add_theme_supoort_apperance_tools": "",
    "background": {
      "backgroundImage": false,
      "backgroundSize": false
    },

    "__not_working_yet": "",
    "alignContent": false,
    "alignText": false,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    }

  }

}