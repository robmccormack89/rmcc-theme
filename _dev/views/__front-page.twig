{% extends "base.twig" %}

{% block header %}

  <!-- header v1 -->
  <header class="uk-inline" hidden>
    
    <div class="header-top-nav uk-position-top uk-position-z-index">
      <div uk-sticky="animation: uk-animation-slide-top; sel-target: .uk-navbar-container; cls-active: uk-navbar-sticky; cls-inactive: uk-navbar-transparent uk-light; top: 200">
        {% include 'header-nav.twig' %}
      </div>
    </div>
    
    <img 
    class="cover-img home-cover-img" 
    data-src="{{post.thumbnail.src|resize(1920)|default(options.site_featured_image.url|resize(1920))|default(theme.link ~ '/assets/images/place1.jpg')|e('esc_url')}}" 
    alt="{{post.thumbnail.alt|default(options.site_featured_image.alt)|e('esc_html')}}" 
    data-width data-height 
    uk-height-viewport 
    uk-img>
    
    <div class="uuk-overlay-primary uk-position-cover">
      <div class="uk-position-top-left uk-overlay uk-overlay-default" hidden>Top Left</div>
      <div class="uk-position-top-center uk-overlay uk-overlay-default" hidden>Top Center</div>
      <div class="uk-position-top-right uk-overlay uk-overlay-default" hidden>Top Right</div>
      <div class="uk-position-center-left uk-overlay uk-overlay-default" hidden>Center Left</div>
      <div class="uk-position-center">
        <div class="uk-text-center uk-container uk-container-xsmall uk-padding">
          <h1 class="uk-margin-large-top">
            {{ __('Lorem ipsum dolor sit amet consectetur', 'serchek-theme')|e('esc_html') }}
