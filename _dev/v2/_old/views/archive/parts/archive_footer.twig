{% set posts = archive.posts %}
{% if (posts.pagination.next or posts.pagination.prev) %}
  <footer class="archive-footer">
    {% include 'pagination.twig' with { pagination: posts.pagination({show_all: false, mid_size: 3, end_size: 2}), container_class: 'rmcc-section-small rmcc-padding-remove-bottom rmcc-margin-remove' } %}
  </footer>
{% endif %}