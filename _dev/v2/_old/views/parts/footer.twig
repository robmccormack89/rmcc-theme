<footer class="site-footer rmcc-background-muted" data-template="footer.twig">
  <div class="rmcc-container rmcc-padding">

    {# footer socials/icon nav #}
    {% include '_menu-iconnav.twig' %}

    {# divider (but only if iconnav is in use) #}
    {% if has_menu_iconnav %}
      <hr class="rmcc-divider-small rmcc-text-center">
    {% endif %}

    {# footer scroll to top button #}
    <div class="rmcc-flex rmcc-flex-center">
      <a href="#" rmcc-totop rmcc-scroll rmcc-tooltip="{{ _x('Scroll to Top', 'Footer: to top', 'basic-theme')|e('esc_html') }}"></a>
    </div>

    {# footer copyrights & credits #}
    {% include '_footer-copyright.twig' %}

  </div>
</footer>